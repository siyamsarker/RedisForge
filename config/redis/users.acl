user default on >${REDIS_REQUIREPASS} allkeys allchannels allcommands ~* &* +@all
user ${REDIS_ACL_USER} on >${REDIS_ACL_PASS} ~* &* +@all -@dangerous -@admin -CONFIG -SLAVEOF -REPLICAOF -SAVE -BGSAVE -BGREWRITEAOF -SHUTDOWN -DEBUG -FLUSHDB -FLUSHALL -KEYS -MIGRATE -RESTORE -RESTORE-ASKING -MODULE -MONITOR -ROLE -ACL
user ${REDIS_READONLY_USER} on >${REDIS_READONLY_PASS} ~* +@read +@connection +PING +INFO +CLIENT +CLUSTER -@write -@dangerous -@admin
user ${REDIS_MONITOR_USER} on >${REDIS_MONITOR_PASS} ~* +PING +INFO +CLIENT|LIST +CLIENT|GETNAME +CLIENT|SETNAME +CONFIG|GET +CLUSTER|INFO +CLUSTER|NODES +SLOWLOG +LATENCY allchannels -@all
user ${REDIS_REPLICATION_USER} on >${REDIS_REPLICATION_PASS} ~* +PSYNC +REPLCONF +PING +INFO +CLIENT +SUBSCRIBE +UNSUBSCRIBE +PSUBSCRIBE +PUNSUBSCRIBE allchannels -@all
